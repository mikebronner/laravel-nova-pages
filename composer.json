{
    "name": "genealabs/laravel-nova-pages",
    "description": "Drop-in functionality for custom pages in Laravel and Nova.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "proprietary",
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        },
        {
            "type": "path",
            "url": "../nova-gutenberg"
        },
        {
            "type": "path",
            "url": "../laravel-overridable-model"
        }
    ],
    "require": {
        "cviebrock/eloquent-sluggable": "*",
        "genealabs/laravel-overridable-model": "*",
        "genealabs/nova-gutenberg": "*",
        "illuminate/routing": "^9.0",
        "illuminate/support": "^9.0",
        "illuminate/view": "^9.0",
        "laravel/nova": "*",
        "symfony/thanks": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "GeneaLabs\\LaravelNovaPages\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "GeneaLabs\\LaravelNovaPages\\Providers\\Service",
                "GeneaLabs\\LaravelNovaPages\\Providers\\Nova"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
